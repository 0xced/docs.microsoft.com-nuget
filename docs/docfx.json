{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "nuget/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "nuget/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
        "globalMetadata": {
            "breadcrumb_path": "~/_breadcrumb/toc.yml",
            "ROBOTS": "INDEX,FOLLOW",
            "searchScope": ["NuGet"]
            },
    "fileMetadata": {},
    "template": [],
    "dest": "nuget"
  }
}